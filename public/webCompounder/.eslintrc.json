{
   "env": {
      "browser": true,
      "es2021": true
   },
   "extends": ["eslint:recommended", "prettier"],
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": ["prettier"],
   "rules": {
      "eqeqeq": "error",
      "no-eval": "error",
      
      "yoda": ["error", "never"],
      "prettier/prettier": "error"
   }
}
